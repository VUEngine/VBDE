<templateSet group="VUEngine Partials">
  <template name=".pitv" value="for(node = VirtualList_begin($VLIST$); node; node = VirtualNode_getNext(node))&#10;{&#10;    $END$&#10;}&#10;" description="VUEngine Partial: VirtualList for loop     " toReformat="false" toShortenFQNames="true">
    <variable name="VLIST" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name=".mnma" value="NM_ASSERT($VAR$, &quot;$CLS$::$FNC$: $MSG$&quot;);&#10;$END$" description="VUEngine Macro: NM_ASSERT                " toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="&quot;this&quot;" alwaysStopAt="true" />
    <variable name="CLS" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="true" />
    <variable name="FNC" expression="" defaultValue="&quot;function&quot;" alwaysStopAt="true" />
    <variable name="MSG" expression="" defaultValue="&quot;null this&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name=".pgti" value="$NAME$::getInstance()$END$" description="VUEngine Partial: get instance method      " toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="&quot;Class&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name=".peed" value="extern EntityDefinition $NAME$;&#10;" description="VUEngine Partial: extern EntityDefinition  " toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="capitalizeAndUnderscore(String)" defaultValue="&quot;ENTITY_EN&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name=".pecd" value="extern CharSetDefinition $NAME$;&#10;" description="VUEngine Partial: extern CharSetDefinition " toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="capitalizeAndUnderscore(String)" defaultValue="&quot;CHARSET_CH&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name=".pdiv" value="//---------------------------------------------------------------------------------------------------------&#10;// &#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;$LABEL$&#10;//---------------------------------------------------------------------------------------------------------&#10;&#10;$END$" description="VUEngine Partial: visual divider           " toReformat="false" toShortenFQNames="true">
    <variable name="LABEL" expression="enum(&quot;ASSETS&quot;, &quot;CLASS'S DEFINITION&quot;, &quot;CLASS'S METHODS&quot;, &quot;DECLARATIONS&quot;, &quot;DEFINITIONS&quot;, &quot;ENTRY POINTS&quot;, &quot;INCLUDES&quot;, &quot;MACROS&quot;, &quot;PROTOTYPES&quot;, &quot;STAGE DEFINITION&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name=".pv3d" value="Vector3D $VAR$ = {&#10;    $X$, &#10;    $Y$, &#10;    $Z$,&#10;};&#10;$END$" description="VUEngine Partial: Vector3D def.            " toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="snakeCase(String)" defaultValue="&quot;position&quot;" alwaysStopAt="true" />
    <variable name="X" expression="" defaultValue="0" alwaysStopAt="true" />
    <variable name="Y" expression="" defaultValue="0" alwaysStopAt="true" />
    <variable name="Z" expression="" defaultValue="0" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name=".pv3dc" value="Vector3D $VAR$ = {$X$, $Y$, $Z$};&#10;$END$" description="VUEngine Partial: Vector3D def. (compact)  " toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="snakeCase(String)" defaultValue="&quot;position&quot;" alwaysStopAt="true" />
    <variable name="X" expression="" defaultValue="0" alwaysStopAt="true" />
    <variable name="Y" expression="" defaultValue="0" alwaysStopAt="true" />
    <variable name="Z" expression="" defaultValue="0" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name=".pfnc" value="$STATIC$$RET$ $CLS$::$FNC$($ARG$)&#10;{&#10;&#9;$END$&#10;}&#10;" description="VUEngine Partial: function definition      " toReformat="true" toShortenFQNames="true">
    <variable name="STATIC" expression="enum(&quot;&quot;, &quot;static &quot;, &quot;inline &quot;, &quot;inline static &quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="RET" expression="enum(&quot;void&quot;, &quot;int&quot;, &quot;bool&quot;, &quot;u8&quot;, &quot;u16&quot;, &quot;u32&quot;, &quot;s8&quot;, &quot;s16&quot;, &quot;s32&quot;, &quot;BYTE*&quot;, &quot;BgmapTexture&quot;, &quot;CharSet&quot;, &quot;Object&quot;, &quot;VBVec3D&quot;, &quot;VBVec3D*&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="CLS" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="true" />
    <variable name="FNC" expression="" defaultValue="&quot;functionName&quot;" alwaysStopAt="true" />
    <variable name="ARG" expression="" defaultValue="&quot;, Additional arguments&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name=".petd" value="extern TextureDefinition $NAME$;&#10;" description="VUEngine Partial: extern TextureDefinition " toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="capitalizeAndUnderscore(String)" defaultValue="&quot;TEXTURE_TX&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name=".pcmd" value="/**&#10; * $END$&#10; *&#10; * @memberof $CLASSNAME$&#10; */" description="VUEngine Partial: comment block (def.)     " toReformat="false" toShortenFQNames="true">
    <variable name="CLASSNAME" expression="capitalize(fileNameWithoutExtension())" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name=".pcmf" value="/**&#10; * $END$&#10; *&#10; * @param $PNAME$&#9;$PDESCRIPTION$&#10; * @return&#9;&#9;&#9;$RETURN$&#10; */" description="VUEngine Partial: comment block (function) " toReformat="false" toShortenFQNames="true">
    <variable name="PNAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PDESCRIPTION" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="RETURN" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
